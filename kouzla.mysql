SHOW DATABASES;
CREATE DATABASE kouzla;
USE kouzla;
SELECT DATABASE();

CREATE TABLE povolani (nazev VARCHAR(20) NOT NULL PRIMARY KEY,) 

CREATE TABLE kouzlo (id INT,
název VARCHAR(20) NOT NULL PRIMARY KEY,
level SMALLINT UNSIGNED, 
povolání ENUM('Bard', 'Druid', 'Klerik', 'Kouzelník', 'Čarodej', 'Černokněžník', 'Warlock'), 
dosah METER, Verbální BOOLEAN, P BOOLEAN, S BOOLEAN, potřeby VARCHAR(40), cena INT, vyvolání ENUM('soustredeni', '-'), vyvolávání TIME, trvání TIME)

 
